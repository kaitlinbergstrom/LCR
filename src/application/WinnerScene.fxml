<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WinnerSceneController">
   <children>
      <MenuBar prefHeight="29.0" prefWidth="800.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="fileClose" mnemonicParsing="false" onAction="#fileCloseClicked" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="newGameButton" layoutX="235.0" layoutY="453.0" mnemonicParsing="false" onAction="#newGameButtonClicked" text="New Game" />
      <Button fx:id="closeGameButton" layoutX="478.0" layoutY="453.0" mnemonicParsing="false" onAction="#fileCloseClicked" text="Close Game" />
      <Label fx:id="congratsLabel" layoutX="277.0" layoutY="66.0" text="CONGRATULATIONS!">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="winnerLabel" alignment="TOP_CENTER" layoutX="181.0" layoutY="127.0" prefHeight="230.0" prefWidth="439.0" text="winnerLabel" textAlignment="CENTER" />
   </children>
</AnchorPane>
